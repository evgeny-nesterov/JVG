public final class Character extends Object {
	private char value;

	public Character(char value) {
		this.value = value;
	}

	public char charValue() {
		return value;
	}

	public String toString() {
		return "" + value;
	}

	public boolean equals(Object o) {
		return o instanceof Character && ((Character) o).value == value;
	}

	public int hashCode() {
		return value;
	}
}
